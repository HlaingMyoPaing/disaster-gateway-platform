{
  "realm": "camel-realm",
  "enabled": true,
  "roles": {
    "realm": [
      {
        "name": "USER"
      },
      {
        "name": "ADMIN"
      }
    ]
  },
  "clients": [
    {
      "clientId": "camel-console",
      "bearerOnly": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "camel-api-secret"
    },
    {
      "clientId": "camel-api",
      "publicClient": true,
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "redirectUris": [
        "http://localhost:*/*"
      ],
      "webOrigins": [
        "http://localhost:*"
      ]
    }
  ],
  "users": [
    {
