      "lastName": "One",
      "realmRoles": [
        "ADMIN"
      ],
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ]
    },
    {
      "username": "user",
      "enabled": true,
      "emailVerified": true,
      "email": "user@example.com",
      "firstName": "User",
      "lastName": "One",
      "realmRoles": [
        "USER"
      ],
      "credentials": [
        {
          "type": "password",
          "value": "user1234",
          "temporary": false
        }
      ]
